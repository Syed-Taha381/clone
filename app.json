{
  "expo": {
    "name": "uberclone",
    "slug": "uberclone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uberclone",
      "config": {
        "googleMapsApiKey": "AIzaSyA_DkA3Hs6aB-EfwFykJehazafRS0ryHl4"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.uberclone",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA_DkA3Hs6aB-EfwFykJehazafRS0ryHl4"
        },
        "permissions": [
          "ACCESS_COARSE_LOCATION",
          "ACCESS_FINR_LOCATION",
          "FOREGROUND_SERVICES"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}